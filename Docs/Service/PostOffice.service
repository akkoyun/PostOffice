[Unit]
Description=PostOffice Hardware API Service
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/root/PostOffice
ExecStart=/usr/local/bin/gunicorn -w 4 -k /root/PostOffice/Docs/Service/gunicorn_conf.py.CustomUvicornWorker PostOffice:PostOffice --bind 0.0.0.0:80 -c /root/PostOffice/Docs/Service/gunicorn_conf.py

[Install]
WantedBy=multi-user.target