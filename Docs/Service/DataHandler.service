[Unit]
Description=PostOffice Data Handler Service with Multiple Workers
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/home/postoffice/PostOffice
ExecStart=/home/postoffice/PostOffice/src/Docs/DataHandler_workers.sh
StandardOutput=inherit
StandardError=inherit
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
